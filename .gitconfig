[user]
	name =
	email =
[core]
	editor = vim
	autocrlf = input
	safecrlf = true
	excludesfile = ~/.gitignore_global
[color]
  ui = true
[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset %C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  co = checkout
  ci = commit
  st = status
  br = branch
  pr = pull --rebase
  rh = reset --hard HEAD
  cdf = clean -df
[push]
  default = upstream
